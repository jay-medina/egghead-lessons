{
  "name": "redux-intro",
  "version": "1.0.0",
  "description": "Lessons from redux videos",
  "main": "index.js",
  "scripts": {
    "build": "run-s clean test webpack:build",
    "clean": "rimraf dist",
    "webpack:build": "webpack",
    "watch": "webpack --watch",
    "start": "webpack-dev-server",
    "test": "run-s test:lint test:unit",
    "test:lint": "tslint -p . -c tslint.json",
    "test:unit": "jest --coverage",
    "watch:test": "jest --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^22.1.3",
    "@types/lodash": "^4.14.104",
    "@types/node-uuid": "0.0.28",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "@types/react-redux": "^5.0.15",
    "@types/react-router": "^4.0.22",
    "@types/react-router-dom": "^4.2.4",
    "css-loader": "^0.28.10",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.4.2",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.2",
    "ts-jest": "^22.4.0",
    "ts-loader": "^3.5.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "collectCoverFrom": [
    "**/*{ts,tsx}",
    "!**/node_modules/**"
  ],
  "dependencies": {
    "lodash": "^4.17.5",
    "node-uuid": "^1.4.8",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2"
  },
  "coverageThreshold": {
    "global": {
      "branches": 10,
      "functions": 10,
      "lines": 10,
      "statements": 10
    }
  }
}
